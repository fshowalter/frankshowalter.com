{
  "name": "frankshowalter.com",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "dev": "astro dev",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "knip": "knip",
    "lint": "eslint  .",
    "lint:fix": "eslint --fix .",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "lint:spelling": "cspell . --no-progress --show-context --show-suggestions",
    "lint:spelling:fix": "rm -rf project-words.txt && echo \"# Project Words - DO NOT TOUCH - npm run lint:spelling:fix\" >> project-words.txt && npm run -s lint:spelling -- --words-only --unique --no-exit-code --no-summary \"**\" | LC_ALL=en_US.UTF-8 sort --ignore-case >> project-words.txt",
    "preview": "astro preview",
    "start": "astro dev",
    "test:coverage": "TEST_COVERAGE=true vitest run src --coverage.all true --coverage",
    "test:update": "vitest -u run src",
    "test:watch": "vitest src",
    "test": "vitest run src",
    "update:data": "node --experimental-strip-types updateData.ts"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/react": "4.4.1",
    "@astrojs/sitemap": "3.6.0",
    "@tailwindcss/vite": "4.1.16",
    "astro": "5.15.3",
    "astro-compressor": "1.2.0",
    "pagefind": "1.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "satori": "0.18.3",
    "sharp": "0.34.5",
    "tailwindcss": "4.1.16",
    "vite": "6.4.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "2.0.35",
    "@eslint/js": "9.39.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/user-event": "14.6.1",
    "@types/estree": "1.0.8",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/eslint-plugin": "1.4.1",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "cspell": "9.3.0",
    "eslint": "9.39.1",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-better-tailwindcss": "3.7.10",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "jsdom": "27.1.0",
    "knip": "5.67.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "sirv": "3.0.2",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "stylelint-order": "7.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "24.11.0",
    "npm": "11.6.2"
  }
