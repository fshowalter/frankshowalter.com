    "eslint-plugin-unicorn": "59.0.0",
    "globals": "16.0.0",
    "jsdom": "26.1.0",
    "knip": "5.51.1",
    "postcss-html": "1.8.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "stylelint": "16.19.1",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "7.0.0",
